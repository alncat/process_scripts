set title "K-L Distance and CCij"
set ytics 0.2 nomirror 
set y2tics 1
set ylabel 'ccij'
set y2label 'k-l'
set xlabel 'atom'
set grid x y2
set key center center title " "
set autoscale y
set autoscale y2
plot "nmbak517_1" using 1:2 w lp pt 5 axes x1y1 t "ccij", "sum_5_1" using 1:3 w lp pt 7 axes x1y2 t "k-l"

